         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  <display-name>Archetype Created Web Application</display-name>
  
  <context-param>
          <param-name>user</param-name>
          <param-value>jeevith</param-value>
  </context-param>
  
  <context-param>
          <param-name>databaseURL</param-name>
          <param-value>jdbc:postgresql://localhost:5432/employee</param-value>
  </context-param>
  <context-param>
          <param-name>password</param-name>
          <param-value>root</param-value>
  </context-param>
  
  <servlet>
      <servlet-name>config1</servlet-name> 
      <servlet-class>edu.tyss.servletcontext.dto.Servlet1</servlet-class>
      <init-param>
            <param-name>pi</param-name>      
            <param-value>3.142</param-value>
      </init-param>
  </servlet>
  <servlet-mapping>
       <servlet-name>config1</servlet-name>
       <url-pattern>servlet1</url-pattern>
  </servlet-mapping>
</web-app>
</element>
