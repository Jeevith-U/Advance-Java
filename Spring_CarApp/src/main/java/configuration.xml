<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="mycar" class="edu.tyss.spring_carapp.model.Car">
		<property name="id" value="22"></property>
		<property name="name" value="RollsRoys"></property>
		<property name="price" value="45000000"></property>
		<property name="color" value="white"></property>
	</bean>
	<bean id="myuser" class="edu.tyss.spring_carapp.model.User">
		<property name="id" value="222"></property>
		<property name="name" value="Jeevith"></property>
		<property name="phno" value="928173916"></property>
		<property name="car" ref="mycar"></property>
	</bean>
</beans>